<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600" width="800" height="600">
  <!-- Background gradients -->
  <defs>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E0F7FA;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="fieldGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#8BC34A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#689F38;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="barnGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#D32F2F;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#B71C1C;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="roofGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#5D4037;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3E2723;stop-opacity:1" />
    </linearGradient>
    <pattern id="grassPattern" patternUnits="userSpaceOnUse" width="30" height="30">
      <path d="M0,30 Q5,20 10,30" stroke="#558B2F" stroke-width="1" fill="none" />
      <path d="M15,30 Q20,15 25,30" stroke="#558B2F" stroke-width="1" fill="none" />
      <path d="M5,30 Q10,18 15,30" stroke="#7CB342" stroke-width="1" fill="none" />
      <path d="M20,30 Q25,20 30,30" stroke="#7CB342" stroke-width="1" fill="none" />
    </pattern>
  </defs>
  
  <!-- Sky -->
  <rect width="800" height="250" fill="url(#skyGradient)" />
  
  <!-- Sun -->
  <circle cx="650" cy="100" r="60" fill="#FFEB3B" />
  <circle cx="650" cy="100" r="50" fill="#FFF176" />
  
  <!-- Distant mountains -->
  <path d="M0,250 L100,180 L200,230 L300,150 L400,200 L500,170 L600,220 L700,160 L800,240 L800,250 Z" fill="#78909C" opacity="0.7" />
  
  <!-- Rolling hills -->
  <path d="M0,250 Q200,200 400,250 T800,230 V300 H0 Z" fill="#8BC34A" />
  <path d="M0,280 Q200,230 400,280 T800,260 V350 H0 Z" fill="#7CB342" />
  
  <!-- Main field -->
  <rect x="0" y="350" width="800" height="250" fill="url(#fieldGradient)" />
  
  <!-- Barn -->
  <g transform="translate(150, 320)">
    <!-- Barn body -->
    <rect x="0" y="0" width="200" height="150" fill="url(#barnGradient)" />
    
    <!-- Barn roof -->
    <polygon points="0,0 100,-50 200,0" fill="url(#roofGradient)" />
    
    <!-- Barn door -->
    <rect x="80" y="70" width="40" height="80" fill="#5D4037" />
    <rect x="98" y="110" width="4" height="10" fill="#FFC107" />
    
    <!-- Barn windows -->
    <rect x="30" y="40" width="30" height="30" fill="#E3F2FD" />
    <rect x="140" y="40" width="30" height="30" fill="#E3F2FD" />
    <line x1="30" y1="55" x2="60" y2="55" stroke="#90A4AE" stroke-width="2" />
    <line x1="45" y1="40" x2="45" y2="70" stroke="#90A4AE" stroke-width="2" />
    <line x1="140" y1="55" x2="170" y2="55" stroke="#90A4AE" stroke-width="2" />
    <line x1="155" y1="40" x2="155" y2="70" stroke="#90A4AE" stroke-width="2" />
    
    <!-- Barn loft window -->
    <circle cx="100" cy="-15" r="15" fill="#E3F2FD" />
    <line x1="100" y1="-30" x2="100" y2="0" stroke="#90A4AE" stroke-width="2" />
    <line x1="85" y1="-15" x2="115" y2="-15" stroke="#90A4AE" stroke-width="2" />
  </g>
  
  <!-- Fence -->
  <g stroke="#8D6E63" stroke-width="5">
    <line x1="400" y1="400" x2="400" y2="450" />
    <line x1="450" y1="400" x2="450" y2="450" />
    <line x1="500" y1="400" x2="500" y2="450" />
    <line x1="550" y1="400" x2="550" y2="450" />
    <line x1="600" y1="400" x2="600" y2="450" />
    <line x1="650" y1="400" x2="650" y2="450" />
    <line x1="700" y1="400" x2="700" y2="450" />
    <line x1="750" y1="400" x2="750" y2="450" />
    <line x1="800" y1="400" x2="800" y2="450" />
    
    <line x1="400" y1="415" x2="800" y2="415" />
    <line x1="400" y1="435" x2="800" y2="435" />
  </g>
  
  <!-- Trees -->
  <g transform="translate(500, 300)">
    <!-- Tree trunk -->
    <rect x="-10" y="0" width="20" height="80" fill="#5D4037" />
    
    <!-- Tree foliage -->
    <circle cx="0" cy="-20" r="40" fill="#388E3C" />
    <circle cx="-25" cy="-10" r="30" fill="#388E3C" />
    <circle cx="25" cy="-10" r="30" fill="#388E3C" />
    <circle cx="0" cy="10" r="35" fill="#388E3C" />
  </g>
  
  <g transform="translate(600, 280)">
    <!-- Tree trunk -->
    <rect x="-8" y="0" width="16" height="60" fill="#5D4037" />
    
    <!-- Tree foliage -->
    <circle cx="0" cy="-15" r="30" fill="#388E3C" />
    <circle cx="-20" cy="-5" r="25" fill="#388E3C" />
    <circle cx="20" cy="-5" r="25" fill="#388E3C" />
    <circle cx="0" cy="5" r="28" fill="#388E3C" />
  </g>
  
  <g transform="translate(700, 320)">
    <!-- Tree trunk -->
    <rect x="-12" y="0" width="24" height="100" fill="#5D4037" />
    
    <!-- Tree foliage -->
    <circle cx="0" cy="-30" r="50" fill="#388E3C" />
    <circle cx="-35" cy="-15" r="40" fill="#388E3C" />
    <circle cx="35" cy="-15" r="40" fill="#388E3C" />
    <circle cx="0" cy="15" r="45" fill="#388E3C" />
  </g>
  
  <!-- Grass pattern overlay -->
  <rect x="0" y="450" width="800" height="150" fill="url(#grassPattern)" />
  
  <!-- Flowers -->
  <g>
    <!-- Yellow flowers -->
    <circle cx="100" cy="500" r="5" fill="#FFEB3B" />
    <circle cx="100" cy="500" r="2" fill="#FFA000" />
    
    <circle cx="130" cy="520" r="5" fill="#FFEB3B" />
    <circle cx="130" cy="520" r="2" fill="#FFA000" />
    
    <circle cx="170" cy="510" r="5" fill="#FFEB3B" />
    <circle cx="170" cy="510" r="2" fill="#FFA000" />
    
    <!-- Pink flowers -->
    <circle cx="350" cy="530" r="5" fill="#E91E63" />
    <circle cx="350" cy="530" r="2" fill="#FFA000" />
    
    <circle cx="380" cy="510" r="5" fill="#E91E63" />
    <circle cx="380" cy="510" r="2" fill="#FFA000" />
    
    <circle cx="420" cy="540" r="5" fill="#E91E63" />
    <circle cx="420" cy="540" r="2" fill="#FFA000" />
    
    <!-- White flowers -->
    <circle cx="550" cy="520" r="5" fill="#FFFFFF" />
    <circle cx="550" cy="520" r="2" fill="#FFA000" />
    
    <circle cx="580" cy="500" r="5" fill="#FFFFFF" />
    <circle cx="580" cy="500" r="2" fill="#FFA000" />
    
    <circle cx="620" cy="530" r="5" fill="#FFFFFF" />
    <circle cx="620" cy="530" r="2" fill="#FFA000" />
  </g>
  
  <!-- Clouds -->
  <g fill="#FFFFFF" opacity="0.8">
    <ellipse cx="100" cy="80" rx="50" ry="30" />
    <ellipse cx="130" cy="70" rx="40" ry="25" />
    <ellipse cx="70" cy="70" rx="40" ry="25" />
    
    <ellipse cx="400" cy="50" rx="60" ry="35" />
    <ellipse cx="430" cy="40" rx="50" ry="30" />
    <ellipse cx="370" cy="40" rx="50" ry="30" />
  </g>
  
  <!-- Silo -->
  <g transform="translate(300, 350)">
    <rect x="-15" y="-80" width="30" height="80" fill="#BDBDBD" />
    <path d="M-15,0 a15,5 0 0,0 30,0" fill="#9E9E9E" />
    <path d="M-15,-80 a15,5 0 0,1 30,0" fill="#E0E0E0" />
  </g>
  
  <!-- Tractor (simplified) -->
  <g transform="translate(450, 500)">
    <!-- Tractor body -->
    <rect x="-30" y="-20" width="60" height="20" fill="#4CAF50" />
    
    <!-- Tractor cabin -->
    <rect x="-20" y="-40" width="30" height="20" fill="#4CAF50" />
    <rect x="-15" y="-35" width="20" height="10" fill="#E3F2FD" />
    
    <!-- Tractor wheels -->
    <circle cx="-20" cy="0" r="10" fill="#212121" />
    <circle cx="-20" cy="0" r="5" fill="#616161" />
    
    <circle cx="20" cy="0" r="15" fill="#212121" />
    <circle cx="20" cy="0" r="7" fill="#616161" />
  </g>
  
  <!-- Butterflies -->
  <g transform="translate(200, 150) scale(0.5)">
    <path d="M0,0 Q10,-10 20,0 Q10,10 0,0" fill="#FFEB3B" />
    <path d="M20,0 Q30,-10 40,0 Q30,10 20,0" fill="#FFEB3B" />
    <line x1="20" y1="0" x2="20" y2="10" stroke="#333333" stroke-width="1" />
  </g>
  
  <g transform="translate(350, 180) scale(0.5)">
    <path d="M0,0 Q10,-10 20,0 Q10,10 0,0" fill="#E91E63" />
    <path d="M20,0 Q30,-10 40,0 Q30,10 20,0" fill="#E91E63" />
    <line x1="20" y1="0" x2="20" y2="10" stroke="#333333" stroke-width="1" />
  </g>
</svg>