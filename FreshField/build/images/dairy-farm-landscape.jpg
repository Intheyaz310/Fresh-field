<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600" width="800" height="600">
  <!-- Background gradients -->
  <defs>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#1E88E5;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#90CAF9;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="fieldGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#7FB069;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#5D8C4F;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="hillGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#8BC34A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#689F38;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="barnGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#D32F2F;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#B71C1C;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="roofGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#5D4037;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3E2723;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="pathGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#D7CCC8;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#BCAAA4;stop-opacity:1" />
    </linearGradient>
    <pattern id="grassPattern" patternUnits="userSpaceOnUse" width="20" height="20">
      <path d="M0,20 Q5,10 10,20" stroke="#558B2F" stroke-width="1" fill="none" />
      <path d="M10,20 Q15,5 20,20" stroke="#558B2F" stroke-width="1" fill="none" />
    </pattern>
  </defs>
  
  <!-- Sky -->
  <rect width="800" height="300" fill="url(#skyGradient)" />
  
  <!-- Sun -->
  <circle cx="600" cy="100" r="60" fill="#FFEB3B" />
  <circle cx="600" cy="100" r="50" fill="#FFF176" />
  
  <!-- Distant mountains -->
  <path d="M0,300 L100,220 L200,280 L300,200 L400,250 L500,180 L600,240 L700,190 L800,280 L800,300 Z" fill="#78909C" opacity="0.6" />
  
  <!-- Rolling hills -->
  <path d="M0,300 Q200,250 400,300 T800,280 V350 H0 Z" fill="url(#hillGradient)" />
  
  <!-- Main field -->
  <rect x="0" y="350" width="800" height="250" fill="url(#fieldGradient)" />
  
  <!-- Path through field -->
  <path d="M300,600 C350,500 450,450 500,350" stroke="url(#pathGradient)" stroke-width="20" fill="none" />
  
  <!-- Barn complex -->
  <g transform="translate(150, 380)">
    <!-- Main barn -->
    <rect x="0" y="0" width="180" height="120" fill="url(#barnGradient)" />
    <polygon points="0,0 90,-60 180,0" fill="url(#roofGradient)" />
    
    <!-- Barn door -->
    <rect x="70" y="40" width="40" height="80" fill="#5D4037" />
    <rect x="88" y="80" width="4" height="10" fill="#FFC107" />
    <path d="M70,40 v80 h40 v-80 z M90,40 v80" fill="none" stroke="#3E2723" stroke-width="2" />
    
    <!-- Barn windows -->
    <rect x="20" y="30" width="30" height="30" fill="#E3F2FD" />
    <rect x="130" y="30" width="30" height="30" fill="#E3F2FD" />
    <line x1="20" y1="45" x2="50" y2="45" stroke="#90A4AE" stroke-width="2" />
    <line x1="35" y1="30" x2="35" y2="60" stroke="#90A4AE" stroke-width="2" />
    <line x1="130" y1="45" x2="160" y2="45" stroke="#90A4AE" stroke-width="2" />
    <line x1="145" y1="30" x2="145" y2="60" stroke="#90A4AE" stroke-width="2" />
    
    <!-- Barn loft window -->
    <circle cx="90" cy="-20" r="15" fill="#E3F2FD" />
    <line x1="90" y1="-35" x2="90" y2="-5" stroke="#90A4AE" stroke-width="2" />
    <line x1="75" y1="-20" x2="105" y2="-20" stroke="#90A4AE" stroke-width="2" />
    
    <!-- Silo -->
    <g transform="translate(220, 60)">
      <rect x="-20" y="-120" width="40" height="120" fill="#BDBDBD" />
      <path d="M-20,0 a20,5 0 0,0 40,0" fill="#9E9E9E" />
      <path d="M-20,-120 a20,5 0 0,1 40,0" fill="#E0E0E0" />
    </g>
    
    <!-- Smaller barn/shed -->
    <g transform="translate(-80, 40)">
      <rect x="0" y="0" width="60" height="80" fill="#C62828" />
      <polygon points="0,0 30,-30 60,0" fill="#5D4037" />
      <rect x="20" y="30" width="20" height="50" fill="#5D4037" />
    </g>
  </g>
  
  <!-- Fence sections -->
  <g stroke="#8D6E63" stroke-width="4">
    <!-- Left fence -->
    <line x1="50" y1="450" x2="50" y2="500" />
    <line x1="80" y1="450" x2="80" y2="500" />
    <line x1="110" y1="450" x2="110" y2="500" />
    <line x1="140" y1="450" x2="140" y2="500" />
    <line x1="170" y1="450" x2="170" y2="500" />
    <line x1="200" y1="450" x2="200" y2="500" />
    <line x1="230" y1="450" x2="230" y2="500" />
    <line x1="260" y1="450" x2="260" y2="500" />
    
    <line x1="50" y1="465" x2="260" y2="465" />
    <line x1="50" y1="485" x2="260" y2="485" />
    
    <!-- Right fence -->
    <line x1="540" y1="450" x2="540" y2="500" />
    <line x1="570" y1="450" x2="570" y2="500" />
    <line x1="600" y1="450" x2="600" y2="500" />
    <line x1="630" y1="450" x2="630" y2="500" />
    <line x1="660" y1="450" x2="660" y2="500" />
    <line x1="690" y1="450" x2="690" y2="500" />
    <line x1="720" y1="450" x2="720" y2="500" />
    <line x1="750" y1="450" x2="750" y2="500" />
    
    <line x1="540" y1="465" x2="750" y2="465" />
    <line x1="540" y1="485" x2="750" y2="485" />
  </g>
  
  <!-- Trees -->
  <g transform="translate(50, 350)">
    <!-- Tree trunk -->
    <rect x="-8" y="0" width="16" height="60" fill="#5D4037" />
    
    <!-- Tree foliage -->
    <circle cx="0" cy="-20" r="35" fill="#388E3C" />
    <circle cx="-20" cy="-10" r="25" fill="#388E3C" />
    <circle cx="20" cy="-10" r="25" fill="#388E3C" />
    <circle cx="0" cy="5" r="30" fill="#388E3C" />
  </g>
  
  <g transform="translate(650, 330)">
    <!-- Tree trunk -->
    <rect x="-10" y="0" width="20" height="80" fill="#5D4037" />
    
    <!-- Tree foliage -->
    <circle cx="0" cy="-25" r="45" fill="#388E3C" />
    <circle cx="-25" cy="-15" r="35" fill="#388E3C" />
    <circle cx="25" cy="-15" r="35" fill="#388E3C" />
    <circle cx="0" cy="10" r="40" fill="#388E3C" />
  </g>
  
  <g transform="translate(750, 360)">
    <!-- Tree trunk -->
    <rect x="-7" y="0" width="14" height="50" fill="#5D4037" />
    
    <!-- Tree foliage -->
    <circle cx="0" cy="-15" r="30" fill="#388E3C" />
    <circle cx="-15" cy="-5" r="20" fill="#388E3C" />
    <circle cx="15" cy="-5" r="20" fill="#388E3C" />
    <circle cx="0" cy="5" r="25" fill="#388E3C" />
  </g>
  
  <!-- Cows in the field -->
  <g transform="translate(400, 500) scale(0.8)">
    <!-- Cow body -->
    <ellipse cx="0" cy="0" rx="40" ry="25" fill="#FFFFFF" stroke="#333333" stroke-width="2" />
    
    <!-- Cow spots -->
    <ellipse cx="-20" cy="-5" rx="10" ry="8" fill="#333333" />
    <ellipse cx="10" cy="8" rx="12" ry="10" fill="#333333" />
    
    <!-- Cow head -->
    <ellipse cx="-45" cy="-10" rx="15" ry="12" fill="#FFFFFF" stroke="#333333" stroke-width="2" />
    
    <!-- Cow ears -->
    <path d="M-55,-18 L-65,-25 L-52,-22 Z" fill="#FFFFFF" stroke="#333333" stroke-width="2" />
    <path d="M-35,-18 L-25,-25 L-38,-22 Z" fill="#FFFFFF" stroke="#333333" stroke-width="2" />
    
    <!-- Cow eyes -->
    <circle cx="-50" cy="-12" r="2" fill="#333333" />
    <circle cx="-40" cy="-12" r="2" fill="#333333" />
    
    <!-- Cow nose -->
    <ellipse cx="-52" cy="-5" rx="5" ry="3" fill="#FFC0CB" />
    
    <!-- Cow legs -->
    <rect x="-30" y="22" width="5" height="20" fill="#FFFFFF" stroke="#333333" stroke-width="1" />
    <rect x="-15" y="22" width="5" height="20" fill="#FFFFFF" stroke="#333333" stroke-width="1" />
    <rect x="10" y="22" width="5" height="20" fill="#FFFFFF" stroke="#333333" stroke-width="1" />
    <rect x="25" y="22" width="5" height="20" fill="#FFFFFF" stroke="#333333" stroke-width="1" />
    
    <!-- Cow tail -->
    <path d="M40,0 Q50,10 55,-10" fill="none" stroke="#333333" stroke-width="2" />
  </g>
  
  <g transform="translate(500, 530) scale(0.7)">
    <!-- Cow body -->
    <ellipse cx="0" cy="0" rx="40" ry="25" fill="#FFFFFF" stroke="#333333" stroke-width="2" />
    
    <!-- Cow spots -->
    <ellipse cx="-20" cy="-5" rx="10" ry="8" fill="#333333" />
    <ellipse cx="10" cy="8" rx="12" ry="10" fill="#333333" />
    
    <!-- Cow head -->
    <ellipse cx="-45" cy="-10" rx="15" ry="12" fill="#FFFFFF" stroke="#333333" stroke-width="2" />
    
    <!-- Cow ears -->
    <path d="M-55,-18 L-65,-25 L-52,-22 Z" fill="#FFFFFF" stroke="#333333" stroke-width="2" />
    <path d="M-35,-18 L-25,-25 L-38,-22 Z" fill="#FFFFFF" stroke="#333333" stroke-width="2" />
    
    <!-- Cow eyes -->
    <circle cx="-50" cy="-12" r="2" fill="#333333" />
    <circle cx="-40" cy="-12" r="2" fill="#333333" />
    
    <!-- Cow nose -->
    <ellipse cx="-52" cy="-5" rx="5" ry="3" fill="#FFC0CB" />
    
    <!-- Cow legs -->
    <rect x="-30" y="22" width="5" height="20" fill="#FFFFFF" stroke="#333333" stroke-width="1" />
    <rect x="-15" y="22" width="5" height="20" fill="#FFFFFF" stroke="#333333" stroke-width="1" />
    <rect x="10" y="22" width="5" height="20" fill="#FFFFFF" stroke="#333333" stroke-width="1" />
    <rect x="25" y="22" width="5" height="20" fill="#FFFFFF" stroke="#333333" stroke-width="1" />
    
    <!-- Cow tail -->
    <path d="M40,0 Q50,10 55,-10" fill="none" stroke="#333333" stroke-width="2" />
  </g>
  
  <g transform="translate(600, 480) scale(0.75)">
    <!-- Cow body -->
    <ellipse cx="0" cy="0" rx="40" ry="25" fill="#FFFFFF" stroke="#333333" stroke-width="2" />
    
    <!-- Cow spots -->
    <ellipse cx="-20" cy="-5" rx="10" ry="8" fill="#333333" />
    <ellipse cx="10" cy="8" rx="12" ry="10" fill="#333333" />
    
    <!-- Cow head -->
    <ellipse cx="-45" cy="-10" rx="15" ry="12" fill="#FFFFFF" stroke="#333333" stroke-width="2" />
    
    <!-- Cow ears -->
    <path d="M-55,-18 L-65,-25 L-52,-22 Z" fill="#FFFFFF" stroke="#333333" stroke-width="2" />
    <path d="M-35,-18 L-25,-25 L-38,-22 Z" fill="#FFFFFF" stroke="#333333" stroke-width="2" />
    
    <!-- Cow eyes -->
    <circle cx="-50" cy="-12" r="2" fill="#333333" />
    <circle cx="-40" cy="-12" r="2" fill="#333333" />
    
    <!-- Cow nose -->
    <ellipse cx="-52" cy="-5" rx="5" ry="3" fill="#FFC0CB" />
    
    <!-- Cow legs -->
    <rect x="-30" y="22" width="5" height="20" fill="#FFFFFF" stroke="#333333" stroke-width="1" />
    <rect x="-15" y="22" width="5" height="20" fill="#FFFFFF" stroke="#333333" stroke-width="1" />
    <rect x="10" y="22" width="5" height="20" fill="#FFFFFF" stroke="#333333" stroke-width="1" />
    <rect x="25" y="22" width="5" height="20" fill="#FFFFFF" stroke="#333333" stroke-width="1" />
    
    <!-- Cow tail -->
    <path d="M40,0 Q50,10 55,-10" fill="none" stroke="#333333" stroke-width="2" />
  </g>
  
  <!-- Grass pattern overlay -->
  <rect x="0" y="550" width="800" height="50" fill="url(#grassPattern)" />
  
  <!-- Flowers -->
  <g>
    <!-- Yellow flowers -->
    <circle cx="80" cy="520" r="4" fill="#FFEB3B" />
    <circle cx="80" cy="520" r="1.5" fill="#FFA000" />
    
    <circle cx="120" cy="540" r="4" fill="#FFEB3B" />
    <circle cx="120" cy="540" r="1.5" fill="#FFA000" />
    
    <circle cx="160" cy="530" r="4" fill="#FFEB3B" />
    <circle cx="160" cy="530" r="1.5" fill="#FFA000" />
    
    <!-- Pink flowers -->
    <circle cx="700" cy="550" r="4" fill="#E91E63" />
    <circle cx="700" cy="550" r="1.5" fill="#FFA000" />
    
    <circle cx="730" cy="530" r="4" fill="#E91E63" />
    <circle cx="730" cy="530" r="1.5" fill="#FFA000" />
    
    <circle cx="760" cy="560" r="4" fill="#E91E63" />
    <circle cx="760" cy="560" r="1.5" fill="#FFA000" />
    
    <!-- White flowers -->
    <circle cx="300" cy="540" r="4" fill="#FFFFFF" />
    <circle cx="300" cy="540" r="1.5" fill="#FFA000" />
    
    <circle cx="330" cy="520" r="4" fill="#FFFFFF" />
    <circle cx="330" cy="520" r="1.5" fill="#FFA000" />
    
    <circle cx="360" cy="550" r="4" fill="#FFFFFF" />
    <circle cx="360" cy="550" r="1.5" fill="#FFA000" />
  </g>
  
  <!-- Clouds -->
  <g fill="#FFFFFF" opacity="0.8">
    <ellipse cx="150" cy="100" rx="60" ry="35" />
    <ellipse cx="180" cy="90" rx="50" ry="30" />
    <ellipse cx="120" cy="90" rx="50" ry="30" />
    
    <ellipse cx="450" cy="70" rx="70" ry="40" />
    <ellipse cx="480" cy="60" rx="60" ry="35" />
    <ellipse cx="420" cy="60" rx="60" ry="35" />
    
    <ellipse cx="700" cy="120" rx="50" ry="30" />
    <ellipse cx="730" cy="110" rx="40" ry="25" />
    <ellipse cx="670" cy="110" rx="40" ry="25" />
  </g>
  
  <!-- Windmill -->
  <g transform="translate(550, 380)">
    <!-- Windmill base -->
    <polygon points="-15,0 15,0 10,80 -10,80" fill="#BDBDBD" />
    
    <!-- Windmill head -->
    <circle cx="0" cy="0" r="10" fill="#9E9E9E" />
    
    <!-- Windmill blades -->
    <g transform="rotate(0)">
      <rect x="-5" y="-60" width="10" height="50" fill="#E0E0E0" />
    </g>
    <g transform="rotate(90)">
      <rect x="-5" y="-60" width="10" height="50" fill="#E0E0E0" />
    </g>
    <g transform="rotate(180)">
      <rect x="-5" y="-60" width="10" height="50" fill="#E0E0E0" />
    </g>
    <g transform="rotate(270)">
      <rect x="-5" y="-60" width="10" height="50" fill="#E0E0E0" />
    </g>
  </g>
  
  <!-- Birds -->
  <g transform="translate(250, 150)">
    <path d="M0,0 Q5,-5 10,0 Q15,-5 20,0" fill="none" stroke="#333333" stroke-width="1" />
  </g>
  
  <g transform="translate(300, 120)">
    <path d="M0,0 Q5,-5 10,0 Q15,-5 20,0" fill="none" stroke="#333333" stroke-width="1" />
  </g>
  
  <g transform="translate(350, 140)">
    <path d="M0,0 Q5,-5 10,0 Q15,-5 20,0" fill="none" stroke="#333333" stroke-width="1" />
  </g>
</svg>